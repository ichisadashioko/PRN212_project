            <Label Content="test features"/>
            <Button Click="btn_2_1_window_Click" Name="btn_2_1_window" Content="2.1 Quan ly thong tin cu dan"/>
            <Button Name="RegistrationsWindow1_btn" Content="RegistrationsWindow1" Click="RegistrationsWindow1_btn_Click"/>

